#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/runner/work/proc-app/proc-app/out/node_modules/.pnpm/stylelint@16.17.0_typescript@5.9.2/node_modules/stylelint/bin/node_modules:/home/runner/work/proc-app/proc-app/out/node_modules/.pnpm/stylelint@16.17.0_typescript@5.9.2/node_modules/stylelint/node_modules:/home/runner/work/proc-app/proc-app/out/node_modules/.pnpm/stylelint@16.17.0_typescript@5.9.2/node_modules:/home/runner/work/proc-app/proc-app/out/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/runner/work/proc-app/proc-app/out/node_modules/.pnpm/stylelint@16.17.0_typescript@5.9.2/node_modules/stylelint/bin/node_modules:/home/runner/work/proc-app/proc-app/out/node_modules/.pnpm/stylelint@16.17.0_typescript@5.9.2/node_modules/stylelint/node_modules:/home/runner/work/proc-app/proc-app/out/node_modules/.pnpm/stylelint@16.17.0_typescript@5.9.2/node_modules:/home/runner/work/proc-app/proc-app/out/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../node_modules/.pnpm/stylelint@16.17.0_typescript@5.9.2/node_modules/stylelint/bin/stylelint.mjs" "$@"
else
  exec node  "$basedir/../../../../node_modules/.pnpm/stylelint@16.17.0_typescript@5.9.2/node_modules/stylelint/bin/stylelint.mjs" "$@"
fi
